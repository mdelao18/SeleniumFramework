<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Account Tests">
    <test name="Login Tests - Chrome">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="selenium.TestAccount"></class>
        </classes>
    </test>

    <test name="Login Tests - Firefox">
        <parameter name="browser" value="firefox"/>
        <parameter name="searchCriteria" value="iphone"/>
        <parameter name="expectedResult" value="1"></parameter>
        <classes>
            <class name="selenium.TestAccount"></class>
        </classes>
    </test>

    <test name="Login Tests - Internet Explorer">
        <parameter name="browser" value="IE"></parameter>
        <classes>
            <class name="selenium.TestAccount"></class>
        </classes>
    </test>

</suite>